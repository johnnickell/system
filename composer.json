{
    "name": "novuso/system",
    "description": "Novuso System library",
    "license": "MIT",
    "require": {
        "php": ">=7.3"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "0.*",
        "consolidation/robo": "^1.0",
        "mikey179/vfsstream": "^1.0",
        "mockery/mockery": "^1.1",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Novuso\\System\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Novuso\\System\\Test\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
