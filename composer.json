{
    "name": "novuso/system",
    "description": "System library for the Novuso Framework",
    "license": "MIT",
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "0.4.*",
        "fzaninotto/faker": "1.*",
        "mikey179/vfsStream": "1.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "6.*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Novuso\\System\\": "src",
            "Novuso\\Test\\System\\TestCase\\": "test/TestCase",
            "Novuso\\Test\\System\\Traits\\": "test/Traits"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Novuso\\Test\\System\\Collection\\": "test/Collection",
            "Novuso\\Test\\System\\Resources\\": "test/Resources",
            "Novuso\\Test\\System\\Serialization\\": "test/Serialization",
            "Novuso\\Test\\System\\Type\\": "test/Type",
            "Novuso\\Test\\System\\Utility\\": "test/Utility"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
