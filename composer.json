{
    "name": "novuso/system",
    "description": "System library for the Novuso Framework",
    "license": "MIT",
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.6",
        "mikey179/vfsStream": "~1.6",
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.4",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.6"
    },
    "autoload": {
        "psr-4": {
            "Novuso\\System\\": "src",
            "Novuso\\Test\\System\\": "test"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
